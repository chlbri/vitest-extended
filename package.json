{
  "name": "@bemedev/vitest-extended",
  "version": "0.4.0",
  "description": "Extensions to build tests in jest",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "scripts": {
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "test": "pnpm run test:coverage --no-coverage",
    "test:coverage": "vitest run --passWithNoTests",
    "rollup": "rollup -c",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test && pnpm run build",
    "ci:online": "pnpm run config && pnpm run lint && pnpm run test && pnpm run build",
    "ci:admin": "pnpm run rm && pnpm run ci:online",
    "test:watch": "vitest --passWithNoTests",
    "rm": "rm -rf node_modules pnpm-lock.yaml",
    "dep": "pnpm i",
    "dep:off": "pnpm run dep --offline",
    "rinit": "pnpm run rm && pnpm run dep",
    "rinit:off": "pnpm run rinit --offline",
    "tsup": "tsup",
    "build": "pnpm run lib:rm && pnpm run rollup",
    "lib:rm": "rm -rf lib",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint ",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "test:cache:reset": "watchman watch-del '/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/usefull-functions' ; watchman watch-project '/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/usefull-functions'"
  },
  "keywords": [
    "jest",
    "test",
    "test.each",
    "loop",
    "Test acceptation",
    "describe",
    "expect"
  ],
  "files": [
    "lib",
    "public"
  ],
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "Charles-LÃ©vi BRI",
    "url": "https://cv-chlbri-bemedev.vercel.app"
  },
  "license": "MIT",
  "devDependencies": {
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-cov-exclude": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.5.2",
    "dequal": "^2.0.3",
    "eslint": "^8.57.0",
    "glob": "^10.4.1",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rollup": "^4.18.0",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rxjs": "^7.8.1",
    "terser": "^5.30.3",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "xstate": "^4.38.3",
    "zod": "^3.22.5"
  },
  "peerDependencies": {
    "vitest": "^1.5.0"
  }
}
