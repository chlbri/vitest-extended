    "@bemedev/test-extended": "^0.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "dequal": "^2.0.3",
    "eslint": "^9.1.0",
    "glob": "^10.3.12",
    "hotscript": "^1.0.13",
    "jest": "^29.7.0",
    "nanoid": "^5.0.7",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rollup": "^4.16.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "terser": "^5.30.3",
    "ts-jest": "^29.1.2",
